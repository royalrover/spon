{
    "title": "MidProxy接口配置",
    "version": "0.0.1",
    "engine": "mockjs",
    "rulebase": "./extends/midproxy-shop-upgrade/rules/",
    "hotload": 1,
    "status": "mock",
    "interfaces": [ {
        "name": "获取达人店店铺是否升级",
        "id": "Shop.getConditions",
        "method": "get",
        "dataType": "json",
        "encoding": "utf8",
        "isCookieNeeded": false,
        "isRuleStatic": false,
        "ruleFile": "Shop.getConditions.rule.json"
    },{
        "name": "店铺升级",
        "id": "Shop.upgrade",
        "method": "post",
        "dataType": "text",
        "encoding": "utf8",
        "isCookieNeeded": false,
        "isRuleStatic": false,
        "ruleFile": "Shop.upgrade.rule.json"
    },{
        "name": "获取达人店全局信息",
        "id": "Shop.getConfig",
        "method": "get",
        "dataType": "json",
        "encoding": "utf8",
        "isCookieNeeded": false,
        "isRuleStatic": false,
        "ruleFile": "Shop.getConfig.rule.json"
    },{
        "name": "获取商品详细信息",
        "id": "Shop.getItemInfo",
        "method": "get",
        "dataType": "json",
        "encoding": "utf8",
        "isCookieNeeded": true,
        "isRuleStatic": false,
        "ruleFile": "Shop.getItemInfo.rule.json"
    } ]
}