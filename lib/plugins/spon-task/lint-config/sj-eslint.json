{
  "extends": "airbnb",
  "plugins": [
      "react"
  ],
  "rules": {
    "no-var": 0,
    "strict": 0,
    "one-var": 2,
    "func-names": 0,
    "vars-on-top": 0,
    "comma-dangle": 0,
    "comma-spacing": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": 0,
    "no-trailing-spaces": 0,
    "space-before-blocks": 0,
    "eol-last": 0,
    "semi": 1,
    "space-infix-ops": 0,
    "keyword-spacing": 0,
    "new-cap": 0,
    "prefer-template": 0,
    "sj-hook": 2,
    "radix": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "object-shorthand": 0,
    "no-multiple-empty-lines": 0,
    "yoda": 0,
    "wrap-iife": 0,
    "no-with": 0,
    "no-throw-literal": 0,
    "no-script-url": 0,
    "no-return-assign": 0,
    "no-redeclare": 0,
    "no-proto": 0,
    "no-octal-escape": 0,
    "no-octal": 0,
    "no-new-wrappers": 0,
    "no-new-func": 0,
    "no-loop-func": 0,
    "no-iterator": 0,
    "no-implied-eval": 0,
    "no-fallthrough": 0,
    "no-extend-native": 0,
    "no-eval": 0,
    "no-else-return": 0,
    "no-caller": 0,
    "guard-for-in": 0,
    "default-case": 0,
    "consistent-return": 0,
    "block-scoped-var": 0,
    "no-cond-assign": 1,
    "no-control-regex": 0,
    "no-empty-character-class": 0,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-inner-declarations": 0,
    "no-nested-ternary": 0,
    "no-new-object": 0,
    "no-spaced-func": 0,
    "no-unneeded-ternary": 0,
    "object-curly-spacing": 0,
    "padded-blocks": 0,
    "quote-props": 0,
    "key-spacing": 0,
    "no-use-before-define": 1,
    "quotes": 1,
    "eqeqeq": 1,
    "max-len": [1, 120, 2],
    "spaced-comment": 0,
    "keyword-spacing": 0,
    "no-confusing-arrow": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-rest-params": 0,
    "prefer-template": 0,
    "space-in-parens": 0,
    "space-before-blocks": 0,
    "vars-on-top": 0,
    "use-isnan": 2,
    "no-undef": 1,
    "no-undef": 1
  },
  "globals": {
    "jQuery": true,
    "$": true,
    "require": true,
    "wx": true,
    "template": true,
    "Zepto": true,
    "WeixinJSBridge": true
  }
}